@prefix dct:                                                                                    <http://purl.org/dc/terms/> .
@prefix foaf:                                                                                   <http://xmlns.com/foaf/0.1/> .
@prefix ldp:                                                                                    <http://www.w3.org/ns/ldp#> .
@prefix org:                                                                                    <http://www.w3.org/ns/org#> .
@prefix owl:                                                                                    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:                                                                                    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:                                                                                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:                                                                                   <http://www.w3.org/2004/02/skos/core#> .
@prefix time:                                                                                   <http://www.w3.org/2006/time#> .
@prefix vann:                                                                                   <http://purl.org/vocab/vann/> .
@prefix voaf:                                                                                   <http://purl.org/vocommons/voaf#> .
@prefix void:                                                                                   <http://rdfs.org/ns/void#> .
@prefix xsd:                                                                                    <http://www.w3.org/2001/XMLSchema#> .

@prefix nrd:                                                                                    <https://www.nicos-rd.com/> .

@prefix fua:                                                                                    <https://www.nicos-rd.com/fua#> .


fua:EventEmitter
    a                owl:Class ;
#    rdfs:subClassOf  ldp:BasicContainer ;
    skos:notation    "EventEmitter"^^xsd:string ;
    rdfs:label       "Event Emitter"@en ;
    rdfs:isDefinedBy nrd: ;
.

fua:Subject ## abstract
    a                owl:Class ;
    rdfs:subClassOf  fua:EventEmitter ;
#    rdfs:subClassOf  ldp:BasicContainer ;
    skos:notation    "Subject"^^xsd:string ;
    rdfs:label       "Subject"@en ;
    rdfs:isDefinedBy nrd: ;
.

fua:Property # REM : abstract
    a                owl:Class ;
    rdfs:subClassOf  fua:EventEmitter ;
#    rdfs:subClassOf  ldp:Resource ;
    skos:notation    "Property"^^xsd:string ;
    rdfs:label       "Property"@en ;
    rdfs:isDefinedBy nrd: ;
.

fua:DatatypeProperty
    a                owl:Class ;
    rdfs:subClassOf  fua:Property ;
#    rdfs:subClassOf  ldp:Resource ;
    skos:notation    "DatatypeProperty"^^xsd:string ;
    rdfs:label       "Datatype Property"@en ;
    fua:valueType    owl:DatatypeProperty ;
    rdfs:isDefinedBy nrd: ;
.

fua:ObjectProperty
    a                owl:Class ;
    rdfs:subClassOf  fua:Property ;
#    rdfs:subClassOf  ldp:Resource ;
    skos:notation    "ObjectProperty"^^xsd:string ;
    rdfs:label       "Object Property"@en ;
    fua:valueType    owl:ObjectProperty ;
    rdfs:isDefinedBy nrd: ;
.

## TODO : wäre fua:Subject ein ldp:Container, könnte dann vielleicht contains genommen werden?
fua:property
    a                owl:ObjectProperty ;
    skos:notation    "property"^^xsd:string ;
    rdfs:label       "Property"@en ;
    dct:description  """All properties of given Subject."""@en ;
    rdfs:domain      fua:Subject ;
    rdfs:range       fua:DatatypeProperty ;
    rdfs:range       fua:ObjectProperty ;
    rdfs:isDefinedBy nrd: ;
.

fua:serverTimestamp
    a                  owl:DatatypeProperty ;
    rdfs:subPropertyOf fua:timestamp ;
    skos:notation      "serverTimestamp"^^xsd:string ;
    rdfs:label         "Server Timestamp"@en ;
    rdfs:domain        fua:Subject ;
    rdfs:domain        fua:Property ;
    rdfs:range         xsd:dateTimeStamp ;
    rdfs:isDefinedBy   nrd: ;
.

fua:sourceTimestamp
    a                  owl:DatatypeProperty ;
    rdfs:subPropertyOf fua:timestamp ;
    skos:notation      "sourceTimestamp"^^xsd:string ;
    rdfs:label         "Source Timestamp"@en ;
    rdfs:domain        fua:Subject ;
    rdfs:domain        fua:Property ;
    rdfs:range         xsd:dateTimeStamp ;
    rdfs:isDefinedBy   nrd: ;
.

fua:valueType
    a                owl:ObjectProperty ;
    skos:notation    "valueType"^^xsd:string ;
    rdfs:label       "Value Type"@en ;
    rdfs:domain      fua:Property ;
    rdfs:range       rdf:Property ;
    rdfs:isDefinedBy nrd: ;
.

fua:value
    a                rdf:Property ;
    skos:notation    "value"^^xsd:string ;
    rdfs:label       "Value"@en ;
    rdfs:domain      fua:Property ;
    rdfs:range       rdfs:Resource ;
    rdfs:isDefinedBy nrd: ;
.

## EOF fua.subject.ttl