@prefix dct:                                                                                    <http://purl.org/dc/terms/> .
@prefix foaf:                                                                                   <http://xmlns.com/foaf/0.1/> .
@prefix org:                                                                                    <http://www.w3.org/ns/org#> .
@prefix owl:                                                                                    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:                                                                                    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:                                                                                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:                                                                                   <http://www.w3.org/2004/02/skos/core#> .
@prefix time:                                                                                   <http://www.w3.org/2006/time#> .
@prefix vann:                                                                                   <http://purl.org/vocab/vann/> .
@prefix voaf:                                                                                   <http://purl.org/vocommons/voaf#> .
@prefix void:                                                                                   <http://rdfs.org/ns/void#> .
@prefix xsd:                                                                                    <http://www.w3.org/2001/XMLSchema#> .

@prefix nrd:                                                                                    <https://www.nicos-rd.com/> .

@prefix fua:                                                                                    <https://www.nicos-rd.com.org/fua#> .

fua:Subject ## abstract
    a                owl:Class ;
    skos:notation    "Subject"^^xsd:string ;
    rdfs:label       "Subject"@en ;
    rdfs:isDefinedBy nrd: ;
.

fua:Property
    a                owl:Class ;
    skos:notation    "Property"^^xsd:string ;
    rdfs:label       "Property"@en ;
    rdfs:isDefinedBy nrd: ;
.

fua:property
    a                owl:ObjectProperty ;
    skos:notation    "property"^^xsd:string ;
    rdfs:label       "Property"@en ;
    dct:description """All properties of given Subject."""@en ;
    rdfs:domain      fua:Subject ;
    rdfs:range       fua:Property ;
    rdfs:isDefinedBy nrd: ;
.

fua:serverTimestamp
    a                  owl:DatatypeProperty ;
    rdfs:subPropertyOf fua:timestamp ;
    skos:notation      "serverTimestamp"^^xsd:string ;
    rdfs:label         "Server Timestamp"@en ;
    rdfs:domain        fua:Subject, fua:Porperty ;
    rdfs:range         xsd:dateTimeStamp ;
    rdfs:isDefinedBy   nrd: ;
.

fua:sourceTimestamp
    a                  owl:DatatypeProperty ;
    rdfs:subPropertyOf fua:timestamp ;
    skos:notation      "sourceTimestamp"^^xsd:string ;
    rdfs:label         "Source Timestamp"@en ;
    rdfs:domain        fua:Subject, fua:Porperty ;
    rdfs:range         xsd:dateTimeStamp ;
    rdfs:isDefinedBy   nrd: ;
.

fua:value
    a                rdf:Property ;
    skos:notation    "value"^^xsd:string ;
    rdfs:label       "Value"@en ;
    rdfs:domain      fua:Porperty ;
    rdfs:range       rdfs:Resource ;
    rdfs:isDefinedBy nrd: ;
.

## EOF fua.subject.ttl