## this
@prefix fua:  <https://www.nicos-rd.com.org/fua#> .

fua:Collaboration
    a                owl:Class ;
    dct:description  "Generic and abstract class of organizational collaboration."@en ;
    rdfs:subClassOf  org:OrganizationalCollaboration ;
# TODO: ???  fuam:abstract    true ;
    rdfs:isDefinedBy fua: ;
.

# region Project

fua:Project
    a                owl:Class ;
    rdfs:label       "Project"@en ;
    rdfs:label       "Projekt"@de ;
    dct:description  "Collaboration with given lifecycle (beginn AND end) and purpose."@en ;
    rdfs:subClassOf  fua:Collaboration ;
    rdfs:isDefinedBy fua: ;
.
# endregion Project

# region WorkingBroup

fua:WorkingGroup
    a                owl:Class ;
    rdfs:label       "Workinggroup"@en ;
    rdfs:label       "Arbeitsgruppe"@de ;
    dct:description  "Collaboration as a working group."@en ;
    rdfs:subClassOf  fua:Collaboration ;
    rdfs:isDefinedBy fua: ;
.
# endregion WorkingBroup

